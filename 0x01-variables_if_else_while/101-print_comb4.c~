#include <stdio.h>

/**
 * main - Entry point
 *
 * Return: Always 0 (Success)
 */
int main(void)
{
int m, n, o;

for (m = 0; m <= 7; m++)
{
for (n = m + 1; n <= 8; n++)
{
for (o = n + 1; o <= 9; o++)
{
putchar((m % 10) + '0');
putchar((n % 10) + '0');
putchar((o % 10) + '0');

if (m == 7 && n == 8 && o == 9)
continue;
putchar(',');
putchar(' ');
}
}
}
putchar('\n');

return (0);
}            
